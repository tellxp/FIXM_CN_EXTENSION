	<annotation>
		<documentation>
		This schema is an extension of the "AircraftDescription" schema of the FIXM core, enriching it with more properties to cover the extension scope.		
		</documentation>
	</annotation>
	<import namespace="http://www.fixm.aero/flight/2.0" schemaLocation="./../core/flight/flight/Flight.xsd"/>
	<import namespace="http://www.fixm.aero/base/2.0" schemaLocation="./../core/base/Base.xsd"/>
	<include schemaLocation="Acdm.xsd"/>
	<complexType name="AcdmAircraftTypeType">
		<annotation>
			<documentation>
				Aircraft Type extension which allows to use the manufacturer and model of the airframe expressed either as an ICAO-approved designator, a text description, or IATA-approved designator.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="fx:AircraftTypeType">
				<sequence>
					<element name="iataModelIdentifier" type="acdm:AcdmIataAircraftIdentifierType" minOccurs="0">
						<annotation>
							<documentation>
								The IATA code of the aircraft type.
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="AcdmAircraftType" type="acdm:AcdmAircraftTypeType" substitutionGroup="fx:AircraftType"/>
</schema>
